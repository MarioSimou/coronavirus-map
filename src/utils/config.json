{
  "cds": "https://coredatascience.herokuapp.com",
  "basemap": {
    "attribution":"Tiles &copy; Esri &mdash; National Geographic, Esri, DeLorme",
    "url":"https://server.arcgisonline.com/ArcGIS/rest/services/NatGeo_World_Map/MapServer/tile/{z}/{y}/{x}",
    "maxZoom": 6,
    "minZoom": 3
  },
  "countriesEndpoint": "https://corona.lmao.ninja/countries",
  "map": {
    "viewport": {
      "center": [0,0],
      "zoom": 3
    },
    "duration": 2,
    "easeLinearity": 0.25,
    "animate": true,
    "maxBounds": [
      [90,90],
      [-90, -90]
     ],
     "dragging": true,
     "touchZoom": true,
     "keyboard": false
  },
  "circleMarker": {
    "style": {
      "highlight": {
        "weight": 2,
        "color": "#fff",
        "dashArray": 5,
        "fillOpacity": 0.7,
        "stroke": true
      },
      "default": {
        "weight": 1,
        "opacity": 1,
        "color": "white",
        "dashArray": 2,
        "fillOpacity": 0.7,
        "stroke": true
      }
    }
  },
  "legends": [
    [
      {"color": "#e41a1c", "value": 1000 },
      {"color": "#377eb8", "value": 500},
      {"color": "#4daf4a", "value": 200},
      {"color": "#984ea3", "value": 100},
      {"color": "#ff7f00", "value": 50},
      {"color": "#ffff33", "value": 0}
    ]
  ]
}