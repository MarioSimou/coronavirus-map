{
  "cds": "https://coredatascience.herokuapp.com",
  "basemap": {
    "attribution":"Tiles &copy; Esri &mdash; National Geographic, Esri, DeLorme",
    "url":"https://server.arcgisonline.com/ArcGIS/rest/services/NatGeo_World_Map/MapServer/tile/{z}/{y}/{x}",
    "maxZoom": 6,
    "minZoom": 3
  },
  "countriesEndpoint": "https://corona.lmao.ninja/countries",
  "map": {
    "useFlyTo": false,
    "viewport": {
      "center": [0,0],
      "zoom": 3
    },
    "duration": 2,
    "easeLinearity": 0.25,
    "animate": true,
    "maxBounds": [
      [90,90],
      [-90, -90]
     ],
     "dragging": true,
     "touchZoom": true,
     "keyboard": false
  },
  "circleMarker": {
    "style": {
      "highlight": {
        "weight": 3,
        "color": "#fff",
        "dashArray": 5,
        "fillOpacity": 0.7,
        "stroke": true
      },
      "default": {
        "weight": 1,
        "opacity": 1,
        "color": "white",
        "dashArray": 2,
        "fillOpacity": 0.7,
        "stroke": true
      }
    }
  },
  "legends": [
    [
      {"color": "#99000d", "value": 1000 },
      {"color": "#cb181d", "value": 500},
      {"color": "#ef3b2c", "value": 100},
      {"color": "#fb6a4a", "value": 50},
      {"color": "#fc9272", "value": 20},
      {"color": "#fcbba1", "value": 0}
    ]
  ],
  "position": {
    "bottomright": {"bottom": 10, "right": 10 },
    "bottomleft": {"bottom": 10, "left": 10},
    "topleft": {"top": 65, "left": 10},
    "topright": {"top": 65, "right": 10}
  },
  "init": {
    "radius": 7.5,
    "label": {"country": {}, "target": null}
  }
}